{
    "builds": [
        {
            "src": "shenai-react/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "shenai-react/dist"
            }
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                { "key": "Cross-Origin-Opener-Policy", "value": "same-origin" },
                {
                    "key": "Cross-Origin-Embedder-Policy",
                    "value": "require-corp"
                }
            ]
        },
        {
            "source": "/sdk/(.*)",
            "headers": [
                {
                    "key": "Cross-Origin-Resource-Policy",
                    "value": "same-origin"
                }
            ]
        }
    ]
}
